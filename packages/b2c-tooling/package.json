{
  "name": "@salesforce/b2c-tooling",
  "version": "0.0.0",
  "description": "Core tooling library for Salesforce Commerce Cloud B2C CLI",
  "author": "Charles Lavery",
  "license": "MIT",
  "repository": "SalesforceCommerceCloud/b2c-cli",
  "keywords": [
    "SFCC",
    "B2C",
    "Commerce Cloud",
    "tooling"
  ],
  "type": "module",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    },
    "./auth": {
      "development": "./src/auth/index.ts",
      "import": {
        "types": "./dist/esm/auth/index.d.ts",
        "default": "./dist/esm/auth/index.js"
      },
      "require": {
        "types": "./dist/cjs/auth/index.d.ts",
        "default": "./dist/cjs/auth/index.js"
      }
    },
    "./instance": {
      "development": "./src/instance/index.ts",
      "import": {
        "types": "./dist/esm/instance/index.d.ts",
        "default": "./dist/esm/instance/index.js"
      },
      "require": {
        "types": "./dist/cjs/instance/index.d.ts",
        "default": "./dist/cjs/instance/index.js"
      }
    },
    "./platform": {
      "development": "./src/platform/index.ts",
      "import": {
        "types": "./dist/esm/platform/index.d.ts",
        "default": "./dist/esm/platform/index.js"
      },
      "require": {
        "types": "./dist/cjs/platform/index.d.ts",
        "default": "./dist/cjs/platform/index.js"
      }
    },
    "./operations/code": {
      "development": "./src/operations/code/index.ts",
      "import": {
        "types": "./dist/esm/operations/code/index.d.ts",
        "default": "./dist/esm/operations/code/index.js"
      },
      "require": {
        "types": "./dist/cjs/operations/code/index.d.ts",
        "default": "./dist/cjs/operations/code/index.js"
      }
    },
    "./operations/jobs": {
      "development": "./src/operations/jobs/index.ts",
      "import": {
        "types": "./dist/esm/operations/jobs/index.d.ts",
        "default": "./dist/esm/operations/jobs/index.js"
      },
      "require": {
        "types": "./dist/cjs/operations/jobs/index.d.ts",
        "default": "./dist/cjs/operations/jobs/index.js"
      }
    },
    "./operations/sites": {
      "development": "./src/operations/sites/index.ts",
      "import": {
        "types": "./dist/esm/operations/sites/index.d.ts",
        "default": "./dist/esm/operations/sites/index.js"
      },
      "require": {
        "types": "./dist/cjs/operations/sites/index.d.ts",
        "default": "./dist/cjs/operations/sites/index.js"
      }
    },
    "./cli": {
      "development": "./src/cli/index.ts",
      "import": {
        "types": "./dist/esm/cli/index.d.ts",
        "default": "./dist/esm/cli/index.js"
      },
      "require": {
        "types": "./dist/cjs/cli/index.d.ts",
        "default": "./dist/cjs/cli/index.js"
      }
    },
    "./logging": {
      "development": "./src/logging/index.ts",
      "import": {
        "types": "./dist/esm/logging/index.d.ts",
        "default": "./dist/esm/logging/index.js"
      },
      "require": {
        "types": "./dist/cjs/logging/index.d.ts",
        "default": "./dist/cjs/logging/index.js"
      }
    }
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist",
    "specs"
  ],
  "scripts": {
    "generate:types": "openapi-typescript specs/data-api.json -o src/clients/ocapi.generated.ts && openapi-typescript specs/slas-admin-v1.yaml -o src/clients/slas-admin.generated.ts",
    "build": "pnpm run generate:types && pnpm run build:esm && pnpm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "clean": "shx rm -rf dist",
    "lint": "eslint",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "test": "echo \"Error: no test specified\" && exit 0",
    "posttest": "pnpm run lint"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/core": "^4",
    "@oclif/prettier-config": "^0.2.1",
    "@salesforce/dev-config": "^4.3.2",
    "@types/node": "^18.19.130",
    "eslint": "^9",
    "eslint-config-prettier": "^10",
    "eslint-plugin-prettier": "^5.5.4",
    "openapi-typescript": "^7.10.1",
    "prettier": "^3.6.2",
    "shx": "^0.3.3",
    "typescript": "^5",
    "typescript-eslint": "^8"
  },
  "peerDependencies": {
    "@oclif/core": "^4"
  },
  "peerDependenciesMeta": {
    "@oclif/core": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=22.16.0"
  },
  "dependencies": {
    "i18next": "^25.6.3",
    "openapi-fetch": "^0.15.0",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2"
  }
}
